{
  "buildCommand": "npm install --prefix client && npm run build:production --prefix client",
  "outputDirectory": "client/dist",
  "installCommand": "npm install --prefix client",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://focusflow-backend-jitt.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
