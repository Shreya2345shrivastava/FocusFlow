{
  "version": 2,
  "name": "focusflow",
  "buildCommand": "cd client && npm install && npm run build:production",
  "outputDirectory": "client/dist",
  "installCommand": "npm install",
  "framework": null,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://focusflow-backend-jitt.onrender.com/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
